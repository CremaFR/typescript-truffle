{
  "name": "typescript-truffle",
  "version": "0.0.1",
  "description": "Awesome chat application (with ninjas!)",
  "browserslist": [
    "last 2 version",
    "ie 11"
  ],
  "config": {
    "devHost": "localhost",
    "devPort": 3000
  },
  "scripts": {
    "build": "NODE_ENV=production tsc && NODE_ENV=production webpack",
    "build:dev": "NODE_ENV=development tsc",
    "start": "webpack-dev-server",
    "cover": "NODE_ENV=test nyc npm test",
    "test": "npm run build:dev && NODE_ENV=test mocha --recursive -r test/jsdom.js -r test/css-loader.js -r ts-node/register test/**/*-test.ts test/**/*-test.tsx"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@ggitlab.dev-abplatform.net:trainings/todo-app.git"
  },
  "keywords": [
    "JavaScript",
    "React",
    "Collaboration",
    "AIFMD"
  ],
  "publishConfig": {
    "registry": "https://repository.dev-abplatform.net.priv/content/repositories/kpmgjs/"
  },
  "author": "KPMG Luxembourg, Technology & Data Analytics",
  "license": "ISC",
